class Solution {
    static int firstElement(int n) {
        // code here
        int mod=1000000007;
        int a=1, b=1;
        int c=a+b;
        int i=1;
        while(i<n){
            a=b;
            b=c;
            c=(a%mod + b%mod)%mod;
            i++;
        }
        return a;
    }
}
