int max=Integer.MIN_VALUE;
        HashMap<Integer,Integer>mp=new HashMap<>();
        for(int i=0;i<arr.length;i++){
        if(mp.containsKey(arr[i])){
            max=Math.max(max,Math.abs(i-mp.get(arr[i])));
            }
        else{
            mp.put(arr[i],i);
        }
        }
         return max;
