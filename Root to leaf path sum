class Solution {
  
    boolean hasPathSum(Node root, int target) {
     
        if (root == null) {
            return false;
        } else if (root.left == null && root.right == null && target - root.data == 0){
            return true;
        }
        
        return hasPathSum(root.left, target - root.data) || hasPathSum(root.right, target - root.data);
    }
}
